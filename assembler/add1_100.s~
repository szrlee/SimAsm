	LOADI	A	0
	LOADI	B	100
	LOADI	C	10
	LOADI	D	0
	LOADI	E	0
	LOADI	F	0
	LOADI	G	0
loop:	ADDI	G	1
	ADD	A	A	G
	LT	G	B
	CJMP	loop
	LOADI	B	0
	LOADI	G	0
loop1:	DIV	F	A	C
	MUL	D	F	C
	SUB	E	A	D
	PUSH	E
	ADD	A	F	B
	ADDI	G	1
	LT	B	A
	CJMP	loop1
loop2:	POP	E
	ADDI	E	48
	OUT	E	15
	ADDI	B	1
	LT	B	G
	CJMP	loop2
	HLT
